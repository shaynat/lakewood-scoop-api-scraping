@using Homework_63.Models
    @model IEnumerable<NewsPost>

    @foreach (NewsPost np in Model)
    {
        <div class="well col-md-6 col-md-offset-3">
            <a href="@np.Url" target="_blank"><h3>@np.Title</h3></a>
            <img src="@np.Image"/>
            <h5>@np.Text</h5>
        </div>
    }


